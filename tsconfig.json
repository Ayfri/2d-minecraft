{
	"compilerOptions": {
		"outDir": "./dist/",
		"target": "ES2020",
		"importsNotUsedAsValues": "remove",
		"sourceMap": true,
		"module": "esnext",
		"noImplicitAny": false,
		"moduleResolution": "Node",
		"pretty": true,
		"typeRoots": ["node_modules/@types", "./typings"]
	},
	"include": ["src/**/*", "typings/*"],
	"typeAcquisition": {
		"enable": true
	}
}
